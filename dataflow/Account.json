{
	"name": "Account",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Contact_Destination",
						"type": "DatasetReference"
					},
					"name": "Contact"
				},
				{
					"dataset": {
						"referenceName": "Case_Destination",
						"type": "DatasetReference"
					},
					"name": "Cases"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> Contact",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Cases",
				"Contact, Cases join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join"
			]
		}
	}
}